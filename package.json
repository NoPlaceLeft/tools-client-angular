{
  "name": "tools-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build --prod && node server.js",
    "build": "ng build",
    "dev": "ng serve",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ghpages-old-production": "ng build --prod --base-href=\"/\" && angular-cli-ghpages --dir dist/tools-angular --cname old.noplaceleft.tools --repo=https://GH_TOKEN@github.com/NoPlaceLeft/tools-client-production.git --name=\"GH_USERNAME\" --email=\"GH_EMAIL\"",
    "ghpages-production": "ng build --prod --base-href=\"/\" && angular-cli-ghpages --dir dist/tools-angular --cname noplaceleft.tools --repo=https://GH_TOKEN@github.com/taethni/npl-tools-client-production.git --name=\"GH_USERNAME\" --email=\"GH_EMAIL\"",
    "ghpages": "ng build --prod --base-href=\"/\" && angular-cli-ghpages --dir dist/tools-angular --cname dev.noplaceleft.tools  --no-silent"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "@npl-shared/(.*)": "<rootDir>/src/app/shared/$1",
      "@npl-core/(.*)": "<rootDir>/src/app/core/$1",
      "@npl-models/(.*)": "<rootDir>/src/app/models/$1",
      "@npl-admin/(.*)": "<rootDir>/src/app/admin/$1",
      "@npl-template": "<rootDir>/src/templates/index",
      "@agm/core": "<rootDir>/src/app/core/testing/agmcore.mock.ts"
    }
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@agm/js-marker-clusterer": "^1.1.0",
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/material-moment-adapter": "^10.2.7",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/platform-server": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@asymmetrik/ngx-leaflet": "^6.0.1",
    "@swimlane/ngx-charts": "^12.0.1",
    "angular-oauth2-oidc": "^8.0.4",
    "core-js": "^3.3.2",
    "d3": "^5.7.0",
    "file-saver": "^2.0.0",
    "i18next": "^17.2.0",
    "i18next-browser-languagedetector": "^4.0.0",
    "jasmine-core": "~3.5.0",
    "jest-preset-angular": "^8.2.1",
    "js-marker-clusterer": "^1.0.0",
    "koa": "^2.6.2",
    "koa-compress": "^3.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "leaflet": "^1.5.1",
    "lodash-es": "^4.17.11",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "ng-bullet": "^1.0.3",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "tslib": "^2.0.0",
    "uuid": "^3.3.2",
    "xlsx": "^0.15.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.1002.2",
    "@angular/cli": "^10.2.2",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@types/d3": "^5.0.1",
    "@types/file-saver": "^2.0.0",
    "@types/googlemaps": "^3.30.16",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.119",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.4",
    "codelyzer": "^5.1.2",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.3.0",
    "rxjs-tslint": "^0.1.6",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7",
    "webpack": "^4.26.1"
  }
}
